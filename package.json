{
  "name": "tiy-github",

  "version": "0.0.7",

  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "mocha js/tests.js",
    "start": "browser-sync start --server --directory --startPath='src/index.html' --files='index.html,js/*.js,css/*.css'",
    "sass": "node-sass --output-style expanded --output src/css/ src/scss/main.scss"

  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShayHall/TIY-GitHub.git"
  },
  "author": "Shay Hall",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ShayHall/TIY-GitHub/issues"
  },
  "homepage": "https://github.com/ShayHall/TIY-GitHub#readme",
  "dependencies": {
    "chai": "^3.2.0",
    "mocha": "^2.3.2",
    "start": "browser-sync start --server --directory --files='index.html, js/*.js, css/*.css'",
    "sass": "node-sass src/scss/main.scss src/css/main.css -- --watch",
    "install:bower": "bower install --save",
    "postinstall:bower": "wiredep --src 'src/*.html'",
    "postinstall": "bower install"
  },
  "devDependencies": {
    "chai": "^3.3.0",
    "mocha": "^2.3.3",
    "bower": "^1.5.3",
    "browser-sync": "^2.9.11",
    "gulp": "^3.9.0",
    "node-sass": "^3.3.3",
    "wiredep": "^2.2.2"
  }
}
